{{title category.name}}
<div class="grid-container">
  <div class="grid-x">
    <div class="cell">
      <h1>{{category.name}}</h1>
    </div>
  </div>
  <div class="grid-x">
    <div class="cell">
      <ul class="menu">
        <li><a>Details</a></li>
        <li><a>Subcategories</a></li>
        <li><a>{{fa-icon "credit-card"}}Expenses</a></li>
      </ul>
    </div>
  </div>
  <div class="grid-x">
    <div class="cell">
      <table class="unstriped">
        <thead>
          <tr>
            <th>Property</th>
            <th>Value</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>UUID</td>
            <td>{{category.id}}</td>
          </tr>
          <tr>
            <td>Name</td>
            <td>{{category.name}}</td>
          </tr>
          <tr>
            <td>Created At</td>
            <td>{{category.created_at}}</td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
