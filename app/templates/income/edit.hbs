{{title "Edit Income"}}
<div class="grid-container">
  <div class="grid-x">
    <div class="cell small-8">
      <h1>Edit Income</h1>
    </div>
    <div class="cell small-4">
      <div class="button-group float-right">
        {{#link-to "income.show" income.id class="secondary button"}}
          Back
        {{/link-to}}
      </div>
    </div>
  </div>
  <div class="grid-x">
    <div class="cell">
      {{forms/income/edit-income income=(changeset income IncomeValidations) householdMembers=householdMembers incomeSaved=(action "incomeSaved") householdMemberName=householdMemberName householdMemberSelected=(action "householdMemberSelected")}}
    </div>
  </div>
</div>
