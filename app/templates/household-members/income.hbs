<div class="container-lg">
  <h1>Household Member - {{this.householdMember.name}}</h1>
  <Menus::HouseholdMemberItem @memberId={{this.householdMember.id}} />
  <PaginatedTable
    @columns={{this.tableColumns}}
    @rows={{this.incomes}}
    @page={{this.page}}
    @numPages={{this.incomes.meta.pages}}
    @sort={{this.sort}}
    @sortDirection={{this.sortDirection}}
    @setPage={{this.setPage}}
    @setSort={{this.setSort}}
  />
</div>
